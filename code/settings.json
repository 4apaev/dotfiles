{

  /* * *             * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
     * * *             * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
      * * *  Files      * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
       * * *  Window     * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
      * * *  Explorer   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
     * * *             * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *             * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * */
  "window.title": "[ ${dirty} ${folderName} ] ${activeEditorMedium}",
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "window.closeWhenEmpty": true,
  "explorer.confirmDelete": true,

  /* * *             * * *   * * *   * * *   * * *   * * *   * * *
    * * *             * * *   * * *   * * *   * * *   * * *   * * *
     * * *  Terminal   * * *   * * *   * * *   * * *   * * *   * * *
    * * *             * * *   * * *   * * *   * * *   * * *   * * *
   * * *             * * *   * * *   * * *   * * *   * * *   * * */
  "terminal.external.osxExec": "iTerm.app",
  "breadcrumbs.enabled": true,

  /* * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
     * * *  Editor   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
   * * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * */
  "editor.tabSize": 2,
  "editor.lineHeight": 14,
  "editor.fontSize": 12,
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code Retina",
  "scm.inputFontFamily": "IBM Plex Mono",
  "editor.inlayHints.fontFamily": "Menlo",
  "editor.inlineSuggest.enabled": true,
  "editor.inlayHints.fontSize": 11,
  "editor.foldingStrategy": "indentation",
  "editor.foldingHighlight": false,
  "editor.unfoldOnClickAfterEndOfLine": true,
  "editor.showFoldingControls": "always",
  "editor.matchBrackets": "near",
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.cursorStyle": "line",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.renderLineHighlight": "gutter",
  "editor.suggestSelection": "first",
  "editor.accessibilitySupport": "off",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleImplementations": "gotoAndPeek",
  "editor.gotoLocation.alternativeReferenceCommand": "editor.action.goToReferences",
  "editor.gotoLocation.alternativeDefinitionCommand": "editor.action.revealDefinition",
  "editor.gotoLocation.alternativeDeclarationCommand": "editor.action.referenceSearch.trigger",
  "editor.gotoLocation.alternativeImplementationCommand": "editor.action.goToImplementation",
  "editor.gotoLocation.alternativeTypeDefinitionCommand": "editor.action.goToTypeDefinition",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false,
    "source.addMissingImports": true,
  },

  /* * *              * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *              * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
     * * *  WorkBench   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *              * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
   * * *              * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * */
  "workbench.tree.indent": 16,
  "workbench.iconTheme": null,
  "workbench.startupEditor": "none",
  "workbench.fontAliasing": "antialiased",
  "workbench.colorTheme": "Oceanic Next (Sublime)",
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.localHistory.mergeWindow": 20,
  "workbench.localHistory.maxFileEntries": 75,
  "workbench.localHistory.maxFileSize": 512,

  /* * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
     * * *  Theme    * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
   * * *           * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * */
  "workbench.colorCustomizations": {
    "gitDecoration.untrackedResourceForeground": "#99c7947c",
    "gitDecoration.addedResourceForeground": "#99C794",
    "gitDecoration.modifiedResourceForeground": "#6BD8FF",
    "gitDecoration.deletedResourceForeground": "#E55555",
    "gitDecoration.ignoredResourceForeground": "#65737ea4",
    "editor.background": "#303841",
    "editor.foreground": "#d5dbe6",
    // "editorCursor.background"      : "#ff0000",
    "editorCursor.foreground": "#40c463",
    "editor.selectionBackground": "#40ad5d31",
    "editorBracketMatch.border": "#30384100",
    "editorBracketMatch.background": "#1f1d21",
    // "editorBracketHighlight.foreground1"   : "#FFCC33",
    // "editorBracketHighlight.foreground2"   : "#33ffa0",
    // "editorBracketHighlight.foreground3"   : "#33ffff",
    // "editorBracketHighlight.foreground4"   : "#c878c8",
    // "editorBracketHighlight.foreground5"   : "#f44073",
    // "editorBracketHighlight.foreground6"   : "#fa1c5b",
    "sideBar.background": "#1d2227",
    "sideBar.border": "#303841",
    "panel.background": "#1d2227",
    "activityBar.background": "#1d2227",
    "activityBar.border": "#303841",
    "statusBar.background": "#282b3c",
    "statusBar.foreground": "#eeffff",
    "titleBar.activeBackground": "#282b3c",
    "titleBar.activeForeground": "#eeefff"
  },

  /* * *            * * *   * * *   * * *   * * *
    * * *            * * *   * * *   * * *   * * *
     * * *  Search    * * *   * * *   * * *   * * *
    * * *            * * *   * * *   * * *   * * *
   * * *            * * *   * * *   * * *   * * */
  "search.maxResults": 20,
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "search.collapseResults": "alwaysCollapse",
  "search.exclude": {
    "dist": true,
    "build": true,
    "coverage": true,
    "node_modules": true,
    ".c8_temp": true,
    ".c8temp": true,
    ".idea": true,
  },

  /* * *        * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *        * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
     * * *  Ext   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
    * * *        * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
   * * *        * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *   * * *
   */
  "docker.enableDockerComposeLanguageService": true,

  /* * *        * * *   * * *   * * *   * * *
    * * *        * * *   * * *   * * *   * * *
     * * *  Git   * * *   * * *   * * *   * * *
    * * *        * * *   * * *   * * *   * * *
   * * *        * * *   * * *   * * *   * * */
  "git.countBadge": "tracked",
  "git.untrackedChanges": "separate",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  /* * *   Git Gutter    * * *   * * */
  "scm.diffDecorationsGutterWidth": 4,
  "scm.diffDecorationsGutterVisibility": "hover",
  "scm.diffDecorationsGutterPattern": {
    "added": true
  },

  /* * *           * * *   * * *   * * *   * * *
    * * *  Intelli  * * *   * * *   * * *   * * *
     * * *  Sense    * * *   * * *   * * *   * * *
    * * *  Code     * * *   * * *   * * *   * * *
   * * *           * * *   * * *   * * *   * * */
  "path-intellisense.extensionOnImport": true,
  "vsintellicode.sql.completionsEnabled": true,
  "vsintellicode.features.python.deepLearning": "enabled",

  /* * *           * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *
     * * *  ESlint   * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *
   * * *           * * *   * * *   * * *   * * */
  "eslint.run": "onSave",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.alwaysShowStatus": true,
  "problems.showCurrentInStatus": true,

  /* * *          * * *   * * *   * * *   * * *
    * * *          * * *   * * *   * * *   * * *
     * * *  Js/Ts   * * *   * * *   * * *   * * *
    * * *          * * *   * * *   * * *   * * *
   * * *          * * *   * * *   * * *   * * */
  "js/ts.implicitProjectConfig.target": "ESNext",
  "js/ts.implicitProjectConfig.strictFunctionTypes": false,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.referencesCodeLens.enabled": false,
  "typescript.implementationsCodeLens.enabled": false,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,

  /* * *           * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *
     * * *  Format   * * *   * * *   * * *   * * *
    * * *           * * *   * * *   * * *   * * *
   * * *           * * *   * * *   * * *   * * */
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",

  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },

  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },

  "typescript.format.semicolons": "remove",
  "javascript.format.semicolons": "remove",
  "yaml.format.singleQuote": true,
  "html.format.indentInnerHtml": true,
  "html.format.maxPreserveNewLines": 1,
  "css.format.spaceAroundSelectorSeparator": true,
  "scss.format.spaceAroundSelectorSeparator": true,

  /* * *   * *   * * *   * * *   * * *   * * *
    * * *         * * *   * * *   * * *   * * *
     * * *  Mix    * * *   * * *   * * *   * * *
    * * *         * * *   * * *   * * *   * * *
   * * * * * * * * * *   * * *   * * *   * * */
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "testing.automaticallyOpenPeekView": "failureAnywhere",
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.keybindingsPerPlatform": false,

  /* * *             * * *   * * *   * * *   * * *
    * * *             * * *   * * *   * * *   * * *
     * * *  Telemetry  * * *   * * *   * * *   * * *
    * * *             * * *   * * *   * * *   * * *
   * * *             * * *   * * *   * * *   * * */
  "redhat.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",
  "editor.minimap.enabled": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.unicodeHighlight.allowedCharacters": {
    "❯": true,
    "❮": true
  },
}

